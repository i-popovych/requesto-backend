{
  "name": "requesto-backend",
  "version": "0.0.1",
  "description": "Backend for the requesto app!",
  "author": "Ivan",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest build && nest start --watch PORT=8080",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "todo": "leasot -x 'src/**/*.ts'",
    "prepare": "husky install",
    "docker:dev": "docker-compose -f docker-compose-dev.yml up",
    "docker:prod": "docker-compose -f docker-compose.yml up",
    "seed:accountReview": "nestjs-command seed:accountReview"
  },
  "dependencies": {
    "@aws-sdk/client-chime": "^3.504.0",
    "@aws-sdk/client-s3": "^3.504.0",
    "@azure/storage-blob": "^12.24.0",
    "@bull-board/api": "^6.5.3",
    "@bull-board/express": "^6.5.3",
    "@bull-board/nestjs": "^6.5.3",
    "@langchain/core": "^0.3.22",
    "@langchain/pinecone": "^0.1.3",
    "@nestjs/bullmq": "^10.2.2",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mapped-types": "*",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/platform-socket.io": "8.4.7",
    "@nestjs/schedule": "^3.0.2",
    "@nestjs/swagger": "^7.1.17",
    "@nestjs/throttler": "^5.1.2",
    "@nestjs/websockets": "8.4.7",
    "@pinecone-database/pinecone": "^4.0.0",
    "@types/multer": "^1.4.11",
    "agenda": "^5.0.0",
    "airtable": "^0.12.2",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "bullmq": "^5.25.6",
    "chargebee-typescript": "^2.31.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "csv-parser": "^3.0.0",
    "date-fns": "^3.4.0",
    "dayjs": "^1.11.9",
    "firebase-admin": "^12.7.0",
    "google-auth-library": "^8.9.0",
    "googleapis": "^140.0.1",
    "jwt-decode": "^3.1.2",
    "langchain": "^0.3.6",
    "mailgun.js": "^10.2.3",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "mongoose": "^7.2.0",
    "morgan": "^1.10.0",
    "nestjs-cls": "^3.6.0",
    "nestjs-command": "^3.1.4",
    "nodemailer": "^6.9.14",
    "openai": "^4.26.1",
    "papaparse": "^5.4.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "ramda": "^0.30.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sanitize-html": "^2.14.0",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^5.3.4",
    "uuidv4": "^6.2.13",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^16.0.0",
    "@types/nodemailer": "^6.4.15",
    "@types/papaparse": "^5.3.14",
    "@types/sanitize-html": "^2.13.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^27.2.5",
    "leasot": "^13.3.0",
    "lint-staged": "^15.2.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": "22.12.0",
    "npm": "10.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs"
    }
  }
}
